{
    "compilerOptions": {
        "module": "commonjs",
        "alwaysStrict":true,
        "importHelpers":true,
        "target": "ES2017",
        "noImplicitAny": false,
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "allowJs": true,
        "preserveConstEnums":true
    }
}
